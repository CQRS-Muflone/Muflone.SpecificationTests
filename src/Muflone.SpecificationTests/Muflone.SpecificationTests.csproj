<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <IsPackable>true</IsPackable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>1.0.0</Version>
    <Copyright>Alessandro Colla and contributors</Copyright>
    <PackageLicenseUrl>https://github.com/Iridio/Muflone.SpecificationTests/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Iridio/Muflone.SpecificationTests/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Iridio/Muflone.SpecificationTests/</RepositoryUrl>
    <PackageTags>Muflone, testing, specification</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <Authors>Alessandro Colla</Authors>
    <Company>Alessandro Colla</Company>
    <Description>Helpers for testing events with Muflone</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CompareNETObjects" Version="4.57.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="2.1.1" />
    <PackageReference Include="Muflone" Version="0.9.0" />
    <PackageReference Include="xunit" Version="2.4.0" />
  </ItemGroup>

</Project>
